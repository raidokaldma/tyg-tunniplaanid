{
  "name": "tyg-tunniplaanid",
  "main": "index.js",
  "compatibility_date": "2025-09-01",
  "assets": {
    "directory": "./public",
    "binding": "ASSETS",
    "run_worker_first": true // Making sure worker runs, so I get to see logs. Without it, assets get served without logging
  },
  "observability": {
    "enabled": true, // Enable logging
    "head_sampling_rate": 1
  },
  "routes": [
    {
      "pattern": "tyg.tuletaevasappi.ee",
      "custom_domain": true
    }
  ]
}